{
  "name": "decor-agent",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3002",
    "dev:tina": "tinacms dev -c \"next dev --port 3002\"",
    "build": "next build",
    "build:tina": "tinacms build && next build",
    "build:local": "tinacms build --local && next build",
    "build:cf": "npx @cloudflare/next-on-pages",
    "deploy": "wrangler pages deploy .vercel/output/static --project-name=decor-agent",
    "preview": "wrangler pages dev .vercel/output/static",
    "start": "next start",
    "lint": "next lint",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:mobile": "playwright test --project='Mobile Chrome'"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "dompurify": "^3.2.5",
    "gray-matter": "^4.0.3",
    "next": "^14.2.28",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.6.0",
    "tinacms": "^2.2.5"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.16",
    "@playwright/test": "^1.48.2",
    "@tinacms/cli": "^2.1.7",
    "@types/dompurify": "^3.0.5",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5",
    "wrangler": "^4.69.0"
  }
}
